<fest:template xmlns:fest="http://fest.mail.ru" context_name="session">
    <fest:var name="logged">session.isLoggedIn</fest:var>
	<div class="wrapper">
	    <h1>GoGo BomberMan</h1>
        <fest:var name="test">session.bool</fest:var>
	    <div class="form">
		    <div class="form__header">BOMBERMAN</div>
		    <div class="form__content">
			    <div class="form__content__label"><p><a href='#game'> game </a></p></div>
                <fest:if test="true">
                    <div class="form__content__label"><p><a href='#login'> Login </a></p></div>
                 </fest:if>
			    <div class="form__content__label"><p><a href='#register'> register </a></p></div>
			    <div class="form__content__label"><p><a href="#scoreboard">Scoreboard</a></p></div>
			</div>
		</div>
    </div>
</fest:template>